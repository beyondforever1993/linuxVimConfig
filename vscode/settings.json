{
    "workbench.editorAssociations": {
        "*.xps": "default"
    },
    "security.workspace.trust.untrustedFiles": "open",
    "editor.stickyScroll.enabled": true,
    "window.zoomLevel": 2,
    "editor.multiCursorModifier": "ctrlCmd",
    "vim.easymotion": true,
    "vim.leader": ";",
    "vim.sneak": true,
    "vim.handleKeys": {


        "<C-d>": true,
        "<C-f>": true,
        "<C-s>": false,
        "<C-z>": false
    },


    // To improve performance"
    "extensions.experimental.affinity": {
    "vscodevim.vim": 1
    },

    "vim.operatorPendingModeKeyBindings": [

    ],
    "vim.digraphs": {

    },
    "vim.showMarksInGutter": true,
    "vim.useSystemClipboard": true,
    "vim.normalModeKeyBindingsNonRecursive": [
         {
      "before": ["<leader>", "c", "h"],
      "commands": [
        {
          "command": "workbench.action.files.revealInOS",
          "when": "editorTextFocus"
        },
        {
          "command": "vscode.open",
          "args": [
            "${fileDirname}/${fileBasenameNoExtension}.h"
          ]
        },
        {
          "command": "workbench.action.splitEditorRight"
        }
      ],
      "args": {
        "override": true
      }
    } ,

    ],

    "vim.insertModeKeyBindings": [
        {
            "before": ["j", "j"],
            "after": ["<Esc>"]
        }
    ],

    "vim.normalModeKeyBindings": [
       {
          "before":["%"],
          "commands": ["editor.action.jumpToBracket"],
          "when": "editorTextFocus"
    },
       {
          "before":["<leader>", "z", "z"],
          "commands": ["editor.action.toggleWordWrap"]
    },
       {
          "before":["<leader>", "t", "t"],
          "commands": ["outline.focus"]
      },
       {
          "before":["<leader>", "w", "o"],
          "commands": [
              "workbench.action.editorLayoutSingle",
          ]
      },
       {
          "before":["<leader>", "w", "s"],
          "commands": [
              "workbench.action.splitEditorDown",
          ]
      },
      {
         "before":["<leader>", "w", "v"],
         "commands": [
             "workbench.action.splitEditorRight",
         ]
      },
      {
         "before":["<leader>", "w", "c"],
         "commands": [
             "workbench.action.closeEditorsInGroup",
         ]
      },
       {
          "before":["<c-h>"],
          "commands": [
              "editor.action.startFindReplaceAction",
          ],
          "when": "editorFocus || editorIsOpen"
      },
       {
          "before":["leader", "e"],
          "commands": [
              ":e!",
          ]
      },
       {
          "before":["leader", "q"],
          "commands": [
              ":q",
          ]
      },
      {
          "before": ["leader", "w", "w"],
          "commands": [
              ":w",
          ]
      },
      {
          "before": ["<C-x>", "g"],
          "commands": [
            "copyRelativeFilePath"
          ],
          "when": "!editorFocus"
      },
      {
          "before": ["<C-x>", "g"],
          "commands": [
            "copyRelativeFilePath"
          ],
          "when": "editorFocus"
      },
      {
          "before": ["<C-x>", "G"],
          "commands": [
              "copyFilePath"
          ],
          "when": "!editorFocus"
      },
      {
          "before": ["<C-x>", "G"],
          "commands": [
              "copyFilePath"
          ],
          "when": "!editorFocus"
      },
      {
          "before": ["<leader>", "d", "d"],
          "commands": [
              "editor.action.revealDefinition"
          ],
          "when": "editorHasDefinitionProvider && editorTextFocus && !isInEmbeddedEditor"
      },
      {
          "before": ["<leader>", "r", "r"],
          "commands": [
              "editor.action.goToReferences"
          ],
          "when": "editorHasReferenceProvider && editorTextFocus && !inReferenceSearchEditor && !isInEmbeddedEditor"
      },
      {
          "before": ["<leader>", "a", "a"],
          "commands": [
              "references-view.findReferences"
          ],
          "when": "editorHasReferenceProvider"
      },
      {
          "before": ["<leader>", "m", "m"],
          "commands": [
              "workbench.action.toggleMaximizeEditorGroup"
          ],
          "when": "maximizedEditorGroup || multipleEditorGroups"
      },

      {
          "before": [
              "<C-n>",
              "a"
          ],
          "after": [
              "\"",
              "a",
              "p",
          ]
      },
      {
          "before": [
              "<C-n>",
              "A"
          ],
          "after": [
              "\"",
              "A",
              "p",
          ]
      },
      {
